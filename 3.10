package com.company;

import java.util.Random;
import java.util.Scanner;

public class Main
{

	public static void main (String[]args)
	{
		Scanner scan=new Scanner(System.in);
		int userInput=scan.nextInt();
		int a;
			for(int i = 0; i <= 10;i++)
			{
				System.out.print(userInput+"*"+i+"=");
				a=userInput*i;
				System.out.println(a);
			}
	}
}
